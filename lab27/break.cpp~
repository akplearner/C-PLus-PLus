#include <iostream>
using namespace std;

int main(){
	cout<<"table of squares"<<endl;
	for(int n=1;n<100;n++){
		if(n>10) break;
		cout<<n<<" "<<n*n<<endl;
	}
	
	 cout<<"table of squares"<<endl;
    for(int n=1;n<100;n++){
        if(n>10) continue;
        cout<<n<<" "<<n*n<<endl;
    }

	return 0;
}
