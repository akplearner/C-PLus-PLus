#include <iostream>
#include <stdlib.h>
#include <time.h>
using namespace std;

void asString(char& a){
	a;
}
void doubleIt(double& a){
	a*2;
}
void doubleThem(int a,int b){
	 a+b;
}

void printArray(bool a,int b,int c){
//	a[b][c]={{true,false,true},{false,true,true}};
}


void randomLetter(){
	srand(time(0));
	int x=rand()%26+1;
	char a,b,c,d,e,f,g,h,i,j,k,l,m,n,o,p,q,r,s,t,u,v,w,x,y,z;
	char L[x]={a,b,c,d,e,f,g,h,i,j,k,l,m,n,o,p,q,r,s,t,u,v,w,x,y,z};
	return L;
}

int main(){
	string s;char c='A';double d=1.1;
	int a[4]={3,1,4,2};
	bool b[2][3]={{true,false,true},{false,true,true}};

	s=asString(c);cout<<s<<endl; 		//prints: A
	doubleIt(d); cout<<d<<endl; 		//prints:2.2
	doubleThem(a,4);cout<<a[0]<<endl;	//prints:6
	printArray(b,2,3);					//prints: TFT FTT
	c=randomLetter();cout<<c<<endl;		

}
