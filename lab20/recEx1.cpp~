#include <iostream>
using namespace std;

int main(){
	int main() {

	int a = 12345, b = 242, c = 135;
	cout << "countEven(" << a << ") = " << countEven(a) << endl;
	cout << "removeEven(" << a << ") = " << removeEven(a) << endl;
	cout << "hasEven(" << a << ") = ";
    if (hasEven(a)) cout << "Yes" << endl << endl;
	else cout << "No" << endl << endl;

	cout << "countEven(" << b << ") = " << countEven(b) << endl;
	cout << "removeEven(" << b << ") = " << removeEven(b) << endl;
	cout << "hasEven(" << b << ") = ";
    if (hasEven(b)) cout << "Yes" << endl << endl;
	else cout << "No" << endl << endl;
	
	cout << "countEven(" << c << ") = " << countEven(c) << endl;
	cout << "removeEven(" << c << ") = " << removeEven(c) << endl;
	cout << "hasEven(" << c << ") = ";
    if (hasEven(c)) cout << "Yes" << endl << endl;
	else cout << "No" << endl << endl;
	return 0;	
}
